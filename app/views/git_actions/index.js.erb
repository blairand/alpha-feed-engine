var github = $('.tab-content').find('.active').find('.github')

<% @git_actions.each do |action| %>
  $(github).prepend(
    $("<%= j render 'dashboard/github_feed_item', action: action %>")
      .hide()
      .fadeIn(3000)
    );
  $(github).children().last().hide();
  // Last hide doesn't really work, and add slide down functionality
<% end %>

GitPoller.poll();